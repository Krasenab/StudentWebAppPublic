        .sections-services {
            padding: 50px 0;
            background-color: #f5f5f5; /* Подходящ фонов цвят :  #f5f5f5; */
            text-align: center;
        }

        .services-containerss h3 {
            font-size: 2.5rem;
            margin-bottom: 30px;
            color: #333; /* Цвят на текста */
        }

        /* Стилове за карточки с услуги */
        .box-icon-classic {
            border:2px solid pink;
            flex-direction:column;
            padding: 30px;
            background: #ffffff;
            margin-bottom: 30px;
            margin-left:6em;
            margin-right:6em;
            box-shadow: 0 4px 25px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s, box-shadow 0.3s; /* Плавен ефект за ховър */
            border-radius: 10px; /* Заоблени ъгли */
        }

            .box-icon-classic:hover {
                transform: translateY(-5px);
                box-shadow: 0 12px 35px rgba(0, 0, 0, 0.2);
            }

        /* Иконки и текст */
        .box-icon-classic-icon {
            font-size: 3rem; /* Размер на иконката */
            color: #17a2b8; /* Цвят на иконката */
            margin-bottom: 20px;
        }

        .box-icon-classic-title {
            font-size: 1.5rem;
            margin-bottom: 15px;
            color: #333; /* Цвят на заглавието */
        }

        .box-icon-classic-text {
            font-size: 1rem;
            color: #777; /* Цвят на текста */
